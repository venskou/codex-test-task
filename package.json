{
  "name": "codex-test-task",
  "version": "0.1.0",
  "private": true,
  "bugs": {
    "url": "https://github.com/venskou/codex-test-task/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/venskou/codex-test-task.git"
  },
  "dependencies": {
    "node-sass": "^4.13.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "3.3.1"
  },
  "devDependencies": {
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "prettier-plugin-packagejson": "^2.0.6"
  },
  "scripts": {
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "lint-staged": "lint-staged",
    "prettier": "prettier --write \"src/**/*.{js,jsx,scss}\"",
    "start": "react-scripts start"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
